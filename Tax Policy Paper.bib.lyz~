@article{sacks2011textquotelefttrafficlighttextquoteright,
	title = {{{\textquoteleft}Traffic-light{\textquoteright}} nutrition labelling and {\textquoteleft}junk-food{\textquoteright} tax: a modelled comparison of cost-effectiveness for obesity prevention},
	volume = {35},
	copyright = {{\textcopyright} 2010 Nature Publishing Group},
	issn = {0307-0565},
	shorttitle = {{{\textquoteleft}Traffic-light{\textquoteright}} nutrition labelling and {\textquoteleft}junk-food{\textquoteright} tax},
	url = {http://www.nature.com.ep.fjernadgang.kb.dk/ijo/journal/v35/n7/full/ijo2010228a.html},
	doi = {10.1038/ijo.2010.228},
	abstract = {Introduction: Cost-effectiveness analyses are important tools in efforts to prioritise interventions for obesity prevention. Modelling facilitates evaluation of multiple scenarios with varying assumptions. This study compares the cost-effectiveness of conservative scenarios for two commonly proposed policy-based interventions: front-of-pack {\textquoteleft}traffic-light{\textquoteright} nutrition labelling (traffic-light labelling) and a tax on unhealthy foods ({\textquoteleft}junk-food{\textquoteright} tax).
Methods: For traffic-light labelling, estimates of changes in energy intake were based on an assumed 10\% shift in consumption towards healthier options in four food categories (breakfast cereals, pastries, sausages and preprepared meals) in 10\% of adults. For the {\textquoteleft}junk-food{\textquoteright} tax, price elasticities were used to estimate a change in energy intake in response to a 10\% price increase in seven food categories (including soft drinks, confectionery and snack foods). Changes in population weight and body mass index by sex were then estimated based on these changes in population energy intake, along with subsequent impacts on disability-adjusted life years ({DALYs).} Associated resource use was measured and costed using pathway analysis, based on a health sector perspective (with some industry costs included). Costs and health outcomes were discounted at 3\%. The cost-effectiveness of each intervention was modelled for the 2003 Australian adult population.
Results: Both interventions resulted in reduced mean weight (traffic-light labelling: 1.3 kg (95\% uncertainty interval ({UI):} 1.2; 1.4); {\textquoteleft}junk-food{\textquoteright} tax: 1.6 kg (95\% {UI:} 1.5; 1.7)); and {DALYs} averted (traffic-light labelling: 45 100 (95\% {UI:} 37 700; 60 100); {\textquoteleft}junk-food{\textquoteright} tax: 559 000 (95\% {UI:} 459 500; 676 000)). Cost outlays were {AUD81} million (95\% {UI:} 44.7; 108.0) for traffic-light labelling and {AUD18} million (95\% {UI:} 14.4; 21.6) for {\textquoteleft}junk-food{\textquoteright} tax. Cost-effectiveness analysis showed both interventions were {\textquoteleft}dominant{\textquoteright} (effective and cost-saving).
Conclusion: Policy-based population-wide interventions such as traffic-light nutrition labelling and taxes on unhealthy foods are likely to offer excellent {\textquoteleft}value for money{\textquoteright} as obesity prevention measures.},
	number = {7},
	urldate = {2013-03-24},
	journal = {International Journal of Obesity},
	author = {Sacks, G. and Veerman, J. L. and Moodie, M. and Swinburn, B.},
	month = jul,
	year = {2011},
	keywords = {cost-effectiveness, food taxes, nutrition labelling, obesity prevention},
	pages = {1001--1009},
	file = {Full Text PDF:/Users/vrangbaek/Library/Application Support/Firefox/Profiles/n41akpa5.default/zotero/storage/KIF8TPS5/Sacks et al. - 2011 - {\textquoteleft}Traffic-light{\textquoteright} nutrition labelling and {\textquoteleft}junk-food.pdf:application/pdf;Snapshot:/Users/vrangbaek/Library/Application Support/Firefox/Profiles/n41akpa5.default/zotero/storage/C3GM7S6M/ijo2010228a.html:text/html}
}

